   "source": [
    "from scipy.stats import zscore\n",
    "\n",
    "def corr(X,Y):\n",
    "    zX = zscore(X)\n",
    "    zY = zscore(Y)\n",
    "    return np.mean(zX*zY)\n",
    "\n",
    "print(corr(X[:,0],X[:,1]))"
   ]
