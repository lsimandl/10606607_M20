    "import numpy as np\n",
    "\n",
    "P_XYZ = np.zeros((2,3,5))\n",
    "\n",
    "for X in range(2):\n",
    "    for Y in range(3):\n",
    "        for Z in range(5):\n",
    "            P_XYZ[X,Y,Z] = (X+1) + (Y+1) + (Z+1)\n",
    "            \n",
    "print(P_XYZ.sum())"
   ]
