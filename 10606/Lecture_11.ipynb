    "from sklearn import datasets\n",
    "import matplotlib.pyplot as plt\n",
    "%matplotlib inline\n",
    "\n",
    "X, y = datasets.make_circles(n_samples=500, factor=.25,\n",
    "                                      noise=.05,)\n",
    "X = X[y==1]\n",
    "plt.scatter(X[:,0],X[:,1])\n",
    "plt.axis('equal');\n",
    "plt.xlabel('x_1')\n",
    "plt.xlabel('x_2')"
   ]
